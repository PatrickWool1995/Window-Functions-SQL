SELECT c.first_name, c.last_name, CONCAT(c.first_name, ' ', c.last_name) AS full_name, date(p.payment_date) as pay_date, p.amount, 
SUM(p.amount) OVER (PARTITION BY CONCAT(c.first_name, ' ', c.last_name) ORDER BY p.payment_date) AS running_payments 
FROM customer AS c LEFT JOIN payment AS p ON c.customer_id = p.customer_id
)
SELECT first_name,last_name, full_name, pay_date, amount, running_payments,LAST_VALUE(running_payments) OVER (PARTITION BY full_name ORDER BY pay_date 
RANGE BETWEEN UNBOUNDED PRECEDING AND UNBOUNDED FOLLOWING) AS total_payment
FROM RunningPayments</sql><current_tab id="0"/></tab_sql></sqlb_project>
